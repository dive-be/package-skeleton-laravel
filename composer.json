{
    "name": "dive-be/:package_name",
    "description": ":package_description",
    "keywords": [
        "dive",
        ":package_name"
    ],
    "homepage": "https://github.com/dive-be/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": "https://dive.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.2",
        "illuminate/console": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dive\\Skeleton\\": "src",
            "Dive\\Skeleton\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "larastan": "vendor/bin/phpstan analyse --memory-limit=2G",
        "format": "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.dist.php --allow-risky=yes",
        "test": "vendor/bin/pest --verbose",
        "verify": "@composer larastan && composer test"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dive\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Dive\\Skeleton\\Facades\\Skeleton"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
